<p id="notice"><%= notice %></p>
<p>
  <strong>Baby:</strong>
  <%= @baby %>
</p>

<h1>Breatsfeeding</h1>

<table>
  <thead>
    <tr>
      <th>Cantidad</th>
      <th>Tiempo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <%  @baby.breatsfeedings.each do |breatsfeeding| %>
      <tr>
        <td><%= breatsfeeding.cantidad %></td>
        <td><%= breatsfeeding.tiempo %></td>
        <td><%= link_to 'Show', baby_breatsfeeding_path(@baby, breatsfeeding) %></td>
        <td><%= link_to 'Edit', edit_baby_breatsfeeding_path(@baby, breatsfeeding) %></td>
        <td><%= link_to 'Destroy', baby_breatsfeeding_path(@baby, breatsfeeding), method: :delete, data: { confirm: 'Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_baby_path(@baby) %> |
<%= link_to 'Back', babies_path %>